#include <winres.h>

VS_VERSION_INFO VERSIONINFO
    FILEVERSION @win32_file_version@
    PRODUCTVERSION @win32_product_version@
    FILEFLAGSMASK 0x3fl
#ifdef _DEBUG
    FILEFLAGS 0x1L
#else
    FILEFLAGS 0x0L
#endif
    FILEOS 0x4004L
    FILETYPE 0x1L
    FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "Crater Crash Studios, LLC"
            VALUE "FileDescription", "@win32_file_description@"
            VALUE "InternalName", "@gde_lib_name@"
            VALUE "OriginalFileName", "@gde_lib_name@"
            VALUE "ProductName", "@win32_product_name@"
            VALUE "ProductVersion", "@win32_product_version@"
            VALUE "FileVersion", "@win32_file_version@"
            VALUE "LegalCopyright", "2023-present Crater Crash Studios LLC and its contributors\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
